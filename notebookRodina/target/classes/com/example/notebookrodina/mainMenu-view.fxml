            <DropShadow offsetX="5.0" offsetY="5.0">
               <color>
                  <Color opacity="0.4848484992980957" />
               </color>
            </DropShadow>
         </effect></Button>
      <ListView fx:id="notesListView" layoutX="300.0" layoutY="25.0" prefHeight="351.0" prefWidth="274.0" style="-fx-background-color: #fdefb0;">
         <effect>
            <InnerShadow>
               <color>
                  <Color opacity="0.48051947355270386" />
               </color>
            </InnerShadow>
         </effect>
      </ListView>
   </children>
</AnchorPane>
