         <font>
            <Font name="Calibri" size="18.0" />
         </font>
      </Label>
      <Label fx:id="nameLabel" layoutX="122.0" layoutY="82.0">
         <font>
            <Font name="Calibri" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="noteTextField" layoutX="48.0" layoutY="157.0" prefHeight="25.0" prefWidth="194.0" style="-fx-background-color: #fad525;">
